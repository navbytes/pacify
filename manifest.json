{
  "manifest_version": 3,
  "name": "__MSG_extName__",
  "version": "1.26.0",
  "description": "__MSG_extDescription__",
  "default_locale": "en",
  "permissions": ["proxy", "storage", "tabs", "notifications", "alarms"],
  "host_permissions": ["<all_urls>"],
  "background": {
    "service_worker": "assets/background.js",
    "type": "module"
  },
  "action": {
    "default_popup": "src/popup/popup.html",
    "chrome_style": false,
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "options_page": "src/options/options.html",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
